<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">


<configuration>
	<!-- DTO(VO)클래스의 별칭 설정 -->
	<typeAliases>
		<!-- mypage관련 DTO클래스 별칭 설정 -->		
		<typeAlias alias="UserInfo" type="music.model.UserInfoVO"/>
		<typeAlias alias="InfoCount" type="music.model.InfoCountVO"/>
		<typeAlias alias="JobInfo" type="music.model.JobInfoVO"/>
		<typeAlias alias="StatusCount" type="music.model.StatusCountVO"/>
		
		<!-- 구인 페이지 관련 -->
		<typeAlias alias="jobBoard" type="music.model.jobBoardVO"/>
		
		<!-- 페이징 처리 -->
		<typeAlias alias="Paging" type="music.model.PagingVO"/>
		
		<!-- 구인 현황 상세페이지 관련 -->
		<typeAlias alias="StatusInfo" type="music.model.StatusInfoVO"/>
		<typeAlias alias="StatusWaitingInfo" type="music.model.StatusWaitingInfoVO"/>
		<typeAlias alias="StatusFinalInfo" type="music.model.StatusFinalInfoVO"/>
		
		<!-- 좋아요 관련 -->
		<typeAlias alias="LikesListComm" type="music.model.LikesListCommVO"/>
		<typeAlias alias="LikesListPR" type="music.model.LikesListPRVO"/>
		<typeAlias alias="PrBoard" type="music.model.PrBoardVO"/>
		
		<!-- 커뮤니티 글 관련 -->
		<typeAlias alias="CommInfo" type="music.model.CommInfoVO"/>
		<typeAlias alias="CommReplyInfo" type="music.model.CommReplyInfoVO"/>
		
		
		<!-- 쪽지함 관련 -->
		<typeAlias alias="MessageInfo" type="music.model.MessageInfoVO"/>
		
		
		<!-- other -->
		
	</typeAliases>
	
	
<!-- <mappers>
		<mapper resource="Board.xml" />
	</mappers> -->
</configuration>

