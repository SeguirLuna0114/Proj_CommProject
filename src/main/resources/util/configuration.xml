<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">


<configuration>
	<!-- DTO(VO)클래스의 별칭 설정 -->
	<typeAliases>
		<!-- pr(자기홍보)관련 DTO클래스 별칭 설정 -->
		<typeAlias alias="pr" type="music.model.prVO" />
		
		<!-- mypage관련 DTO클래스 별칭 설정 -->		
		<!-- 1) member(userId) & prBoard(userId) => UserInfoVO클래스  -->
		<typeAlias alias="UserInfo" type="music.model.UserInfoVO"/>
		
		<!-- 2) jobBoard(jobNo) & jobApp(jobNo) => JobInfoVO클래스  -->
		<typeAlias alias="JobInfo" type="music.model.JobInfoVO"/>

		<typeAlias alias="InfoCount" type="music.model.InfoCountVO"/>
		
		<typeAlias alias="StatusCount" type="music.model.StatusCountVO"/>
		
		
		
	</typeAliases>
	
	
<!-- <mappers>
		<mapper resource="Board.xml" />
	</mappers> -->
</configuration>

